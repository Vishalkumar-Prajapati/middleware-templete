<!DOCTYPE html>
<html lang="en">

<head>
    <title>Document</title>
</head>

<body>
    <table border="2">
        <tr>
            <th>
                UsreName
            </th>
            <th>
                FullName
            </th>
            <th>
                Email
            </th>
            <th>
                PhoneNumber
            </th>
        </tr>
        <%for (const data of database) { %>
            <tr>
                <td>
                    <p><%= data.userName %></p>
                </td>
                <td>
                    <p><%= data.fullName %></p>
                </td>
                <td>
                    <p><%= data.email %></p>
                </td>
                <td>
                    <p><%= data.phoneNumber %></p>
                </td>
            </tr>
        <% } %>

    </table>
</body>

</html>